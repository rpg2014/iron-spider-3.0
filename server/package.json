{
  "name": "iron-spider",
  "version": "0.0.1",
  "description": "The serverless backend implementation for my website",
  "packageManager": "yarn@4.1.0",
  "author": "",
  "license": "Apache-2.0",
  "type": "commonjs",
  "private": true,
  "bin": {
    "cdk": "dist/bin/cdk.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "diff": "cdk diff --profile personal",
    "deploy": "cdk deploy --profile personal --all",
    "format": "yarn prettier . --write",
    "pretest": "yarn build",
    "test": "jest --passWithNoTests",
    "generate-ssdk": "cd ../ && ./gradlew :server-codegen:build",
    "build:client": "cd codegen/client-sdk/build/smithyprojections/typescript-client-codegen/ts-client/typescript-codegen && yarn install && yarn build",
    "build:ssdk": "cd codegen/server-sdk/build/smithyprojections/server-codegen/ts-server/typescript-ssdk-codegen && yarn install && yarn build",
    "regenerate:ssdk": "yarn generate-ssdk && yarn build:ssdk && yarn build:client",
    "build": "yarn regenerate:ssdk && yarn install && yarn test && yarn build:operations ",
    "build:operations": "ts-node configuration/build_operations.ts",
    "prepare": "cd .. && husky install server/.husky"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.458.0",
    "@aws-sdk/client-ec2": "^3.458.0",
    "@aws-sdk/client-route-53": "^3.458.0",
    "@aws-sdk/client-secrets-manager": "^3.458.0",
    "@aws-sdk/client-sesv2": "^3.458.0",
    "@aws-sdk/client-sso-oidc": "^3.620.0",
    "@aws-sdk/lib-dynamodb": "^3.458.0",
    "@aws-smithy/server-apigateway": "^1.0.0-alpha.7",
    "@aws-smithy/server-common": "^1.0.0-alpha.7",
    "@simplewebauthn/server": "^8.3.5",
    "@types/aws-lambda": "^8.10.85",
    "@types/uuid": "^9.0.2",
    "iron-spider-ssdk": "workspace:*",
    "jsonwebtoken": "^9.0.1",
    "jwt-lib": "workspace:^",
    "re2-wasm": "1.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "authorizer": "workspace:^",
    "aws-cdk": "^2.144.0",
    "aws-cdk-lib": "^2.144.0",
    "constructs": "^10.3.0",
    "domain-auth-assets": "workspace:^",
    "esbuild": "^0.19.6",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-export-all": "1.4.1",
    "homelab-aws-infra": "workspace:^",
    "husky": "^7.0.4",
    "jest": "^29.6.2",
    "lint-staged": "^12.1.2",
    "prettier": "2.3.0",
    "remix-site": "workspace:^",
    "rimraf": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.(js|ts)": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.(md|json)": [
      "prettier --write"
    ]
  }
}
