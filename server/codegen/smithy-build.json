{
  "version" : "1.0",
  "outputDirectory" : "build/output",
  "projections" : {
    "ts-server" : {
      "plugins": {
        "typescript-ssdk-codegen" : {
          "package" : "@smithy-demo/iron-spider-service-ssdk",
          "packageVersion": "0.0.1"
        }
      }
    },
    "apigateway" : {
      "imports" : ["model/"],
      "plugins" : {
        "openapi": {
          "service": "com.rpg2014.cloud#IronSpider",
          "protocol": "aws.protocols#restJson1",
          "apiGatewayType" : "REST"
        }
      }
    }
  }
}
