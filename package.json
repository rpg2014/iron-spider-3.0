{
  "name": "iron-spider-build",
  "version": "0.0.1",
  "description": "A yarn project for the iron spider monorepo",
  "packageManager": "yarn@4.3.1",
  "author": "",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build:authorizer": "cd packages/authorizer && yarn build",
    "build:server": "cd server && yarn build",
    "build:domain-auth": "cd packages/domain-auth-assets && yarn build",
    "build:all": "yarn workspaces foreach -ptA run build",
    "build": "yarn generate  && yarn build:server"
  },
  "devDependencies": {
    "@aws-sdk/protocol-http": "^3.374.0",
    "esbuild": "0.22.0",
    "rimraf": "^3.0.0"
  },
  "workspaces": [
    "packages/*",
    "server",
    "server/codegen/client-sdk/build/smithyprojections/typescript-client-codegen/ts-client/typescript-codegen",
    "server/codegen/server-sdk/build/smithyprojections/server-codegen/ts-server/typescript-ssdk-codegen"
  ]
}
