{
  "private": true,
  "name": "remix-site",
  "description": "A remix app implemented on Lambda@Edge using cdk",
  "license": "MIT",
  "version": "1.0",
  "scripts": {
    "build": "yarn build:css && yarn build:rust-functions && remix vite:build && yarn build:sw && echo 'Add or remove build steps to here as needed'",
    "build:css": "sass --no-source-map -s compressed app/ ",
    "build:rust-functions": "wasm-pack build rust-functions --target nodejs --out-name rust-functions",
    "build:sw": "node configuration/build_sw.js",
    "release": "yarn lint && yarn build",
    "dev:css": "sass --watch app/",
    "dev": "remix vite:dev",
    "deploy": "yarn build && cdk deploy --profile personal",
    "diff": "yarn build && cdk diff --profile personal",
    "synth": "cdk synth",
    "postinstall": "remix setup node",
    "start": "remix-serve build/server/index.js",
    "clean": "rm -rf build node_modules rust-functions/pkg",
    "test": "jest .",
    "lint": "yarn prettier . --write && yarn run eslint . --fix"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@remix-run/server-runtime": "^2.8.1",
    "@types/node": "^20.6.0",
    "@types/source-map-support": "^0.5.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "isbot": "latest",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.358.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "remix": "^2.8.1",
    "roadhouse-wasm": "github:rpg2014/roadhouse-wasm",
    "rust-functions": "./rust-functions/pkg",
    "swr": "^2.2.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "aws-cdk": "^2.144.0",
    "aws-cdk-lib": "^2.144.0",
    "concurrently": "^8.2.1",
    "constructs": "^10.3.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.4",
    "prettier": "3.0.3",
    "sass": "^1.67.0",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-wasm": "^3.3.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "packageManager": "yarn@4.1.0"
}
