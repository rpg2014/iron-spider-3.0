    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-wasm": "^3.3.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "packageManager": "yarn@4.3.1"
}
