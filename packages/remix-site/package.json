{
  "private": true,
  "name": "remix-site",
  "description": "A remix app implemented on Lambda@Edge using cdk",
  "license": "MIT",
  "version": "1.0",
  "scripts": {
    "analyze": "yarn build -- --analyze",
    "analyze:vite": "yarn build:wasm && react-router build -- --analyze",
    "build": "yarn build:wasm && react-router build && yarn build:sw && yarn build:deployment_bundle",
    "build:deployment_bundle": "ts-node configuration/build_remix_app.ts",
    "build:docker": "docker build -t remix-site . && docker tag remix-site 192.168.0.222:5000/remix-site",
    "build:css": "echo 'Building CSS' && sass --no-source-map -s compressed app/ && echo 'Done!' ",
    "build:server-rust-functions": "wasm-pack build rust-wasm/server-code --target nodejs --out-name server-rust-functions",
    "build:client-rust-functions": "wasm-pack build rust-wasm/client-code --target web --out-name client-rust-functions",
    "build:wasm": "yarn run build:server-rust-functions && yarn run build:client-rust-functions && yarn install",
    "build:sw": "node configuration/build_sw.js",
    "build-and-deploy": "yarn build && cd .. && yarn deploy:remix-site",
    "build-and-deploy:docker": "yarn build:docker && docker push 192.168.0.222:5000/remix-site && ssh rpg@192.168.0.222 'cd /home/rpg/code/home-infra/compose-files/spider-assistant && docker compose down && docker compose pull --ignore-pull-failures $(docker compose config --services | grep -v app) && docker compose up -d'",
    "build-and-deploy:all": "yarn build-and-deploy && yarn build-and-deploy:docker",
    "release": "yarn lint && yarn build",
    "dev:css": "sass --watch app/",
    "dev:wasm": "nodemon --watch rust-wasm/client-code/pkg/client-rust-functions_bg.wasm --watch node_modules/.vite/deps --exec \"rsync -av --delete rust-wasm/client-code/pkg/client-rust-functions_bg.wasm node_modules/.vite/deps/\"",
    "dev:server": "react-router dev",
    "dev": "concurrently \"yarn dev:wasm\" \"yarn dev:server\"",
    "deploy": "yarn build && cdk deploy --profile personal",
    "diff": "yarn build && cdk diff --profile personal",
    "synth": "cdk synth",
    "start": "USE_STREAMS=true react-router-serve build/server/index.js",
    "start-express": "",
    "clean": "rm -rf build node_modules rust-functions/pkg",
    "test": "jest .",
    "format": "yarn prettier . --write",
    "typecheck": "react-router typegen && tsc",
    "lint": "yarn format && yarn run eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.806.0",
    "@aws-sdk/client-sts": "^3.821.0",
    "@aws-sdk/lib-dynamodb": "^3.806.0",
    "@babylonjs/core": "^8.6.1",
    "@babylonjs/loaders": "^8.6.1",
    "@convex-dev/eslint-plugin": "^0.0.1-alpha.4",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-router/express": "^7.6.0",
    "@react-router/node": "^7.6.0",
    "@react-router/serve": "^7.6.0",
    "@smithy/shared-ini-file-loader": "^4.0.2",
    "@types/node": "^20.6.0",
    "@types/source-map-support": "^0.5.7",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@webgpu/types": "^0.1.60",
    "aws-xray-sdk": "^3.10.1",
    "class-variance-authority": "^0.7.0",
    "client-rust-functions": "./rust-wasm/client-code/pkg",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "compression": "^1.8.0",
    "convex": "^1.23.0",
    "express": "^5.1.0",
    "iron-spider-client": "workspace:^",
    "isbot": "latest",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "lucide-react": "^0.358.0",
    "morgan": "^1.10.0",
    "next-themes": "^0.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router": "^7.6.0",
    "remark-gfm": "^4.0.0",
    "remix": "^2.15.0",
    "roadhouse-wasm": "github:rpg2014/roadhouse-wasm",
    "server-rust-functions": "./rust-wasm/server-code/pkg",
    "sonner": "^2.0.3",
    "swiper": "^11.2.6",
    "swr": "^2.2.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "temporal-polyfill": "^0.2.5",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.6.0",
    "@react-router/fs-routes": "^7.6.0",
    "@remix-run/eslint-config": "^2.15.0",
    "@storybook/addon-docs": "^8.2.4",
    "@storybook/addon-essentials": "^8.2.4",
    "@storybook/addon-interactions": "^8.2.4",
    "@storybook/addon-links": "^8.2.4",
    "@storybook/addon-onboarding": "^8.2.4",
    "@storybook/addon-themes": "^8.2.4",
    "@storybook/blocks": "^8.2.4",
    "@storybook/react": "^8.2.4",
    "@storybook/react-vite": "^8.2.4",
    "@storybook/test": "^8.2.4",
    "@types/aws-lambda": "^8.10.119",
    "@types/compression": "^1",
    "@types/express": "^5",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^9",
    "@types/morgan": "^1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.3.1",
    "aws-cdk": "^2.160.0",
    "aws-cdk-lib": "^2.160.0",
    "concurrently": "^8.2.2",
    "constructs": "^10.3.0",
    "esbuild": "^0.25.0",
    "esbuild-plugin-wasm": "^1.1.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.6.4",
    "nodemon": "^3.1.4",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "rollup-plugin-visualizer": "^5.12.0",
