{
  "name": "domain-auth-assets",
  "private": true,
  "type": "commonjs",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build:Clientside": "yarn build:client && yarn build:server",
    "types": "tsc",
    "build:client": "yarn types && vite build --outDir dist/static",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build": "vite build --outDir dist/static && yarn build:server && node prerender",
    "lint": "eslint  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "yarn prettier . --write",
    "preview": "vite preview",
    "deploy": "cdk"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.95.1",
    "constructs": "^10.2.70"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "20.4.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "aws-cdk-lib": "^2.102.0",
    "compression": "^1.7.4",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.6.2",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.1.6",
    "vite": "^4.4.5"
  }
}
